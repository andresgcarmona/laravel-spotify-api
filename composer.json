{
  "name": "polaris/laravel-spotify-api",
  "description": "A Laravel library wrapper for Spotify's Web API.",
  "require": {
    "php": "^7.3",
    "guzzlehttp/guzzle": "^6.3@dev",
    "nesbot/carbon": "^2.0",
    "ext-json": "*",
    "polaris/laravel-entity": "dev-master"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "orchestra/testbench": "^7.0@dev"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Andres G. Carmona",
      "email": "ing.andres.carmona@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "autoload": {
    "psr-4": {
      "Polaris\\": "src/"
    }
  },
  "repositories": [
    {
      "type": "path",
      "url": "../laravel-entity"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Polaris\\SpotifyApiServiceProvider"
      ]
    }
  }
}
